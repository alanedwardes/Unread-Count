{
	"manifest_version": 2,
	"name": "Unread Count",
	"version": "1.1.7",
	"description": "Shows an unread count over the favicon for sites that support it.",
	"background": {
		"page": "background.html"
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"<all_urls>",
		"notifications"
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png",
		"256": "icon256.png"
	}
}
